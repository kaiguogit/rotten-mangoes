<footer>
  <small>
    <% if current_user %>
      Signed in as <%= current_user.full_name %> (<%= link_to "Log out", session_path, method: :delete %>)
      <%= link_to "My Profile", user_path(current_user) %>
    <% else %>
      <%= link_to "Log In", new_session_path %> | <%= link_to "Sign Up", new_user_path %>
    <% end %>
      <%= link_to "Admin Access", admin_users_path %>
  </small>

</footer>